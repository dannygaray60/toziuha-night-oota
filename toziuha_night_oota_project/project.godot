[display]

window/size/width=683
window/size/height=384
window/size/test_width=1366
window/size/test_height=768
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "screen_debugger" )

