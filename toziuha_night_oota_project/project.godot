config_version=4

_global_script_classes=[ {
"base": "Camera2D",
"class": "ShakeCamera2D",
"language": "GDScript",
"path": "res://objects/PlayerCamera.gd"
} ]
_global_script_class_icons={
"ShakeCamera2D": ""
}

[application]

config/name="Toziuha Night - Order of the Alchemists"
run/main_scene="res://test/level.tscn"
config/icon="res://icon.png"

[autoload]

