; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Joystick",
"language": "GDScript",
"path": "res://addons/Joystick/Joystick.gd"
}, {
"base": "Node",
"class": "PhysicsHelper",
"language": "GDScript",
"path": "res://scripts/PhysicsHelper.gd"
}, {
"base": "Camera2D",
"class": "ShakeCamera2D",
"language": "GDScript",
"path": "res://objects/PlayerCamera.gd"
} ]
_global_script_class_icons={
"Joystick": "",
"PhysicsHelper": "",
"ShakeCamera2D": ""
}

[application]

config/name="Toziuha Night - Order of the Alchemists"
run/main_scene="res://screens/GameInDevNotice.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ToziuhaNight_OrderOfTheAlchemists"
config/icon="res://icon_max_512.png"
config/quit_on_go_back=false

[autoload]

SceneChanger="*res://autoload/SceneChanger.tscn"
ScreenDebugger="*res://addons/screen_debugger/S_ScreenDebugger.gd"
Functions="*res://scripts/functions.gd"
Audio="*res://autoload/Audio.tscn"
Vars="*res://scripts/vars.gd"
Savedata="*res://scripts/savedata.gd"
Timers="*res://autoload/Timers.tscn"
FilterVideo="*res://autoload/FilterVideo.tscn"
ControlsOnscreen="*res://objects/ui/ControlsOnscreen.tscn"
DialogBox="*res://objects/ui/DialogBox.tscn"
JoystickInput="*res://autoload/joystick_input.gd"
Tr="*res://autoload/Tr.gd"

[display]

window/size/width=683
window/size/height=384
window/handheld/orientation="sensor_landscape"
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "screen_debugger", "swipe-detector" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

